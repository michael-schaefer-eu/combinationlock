{
    "name": "michael-schaefer-eu/combinationlock",
    "type": "project",
    "license": "MIT",
    "description": "A combination lock CLI app with docker and phar build",
    "authors": [
        {
            "name": "Michael SchÃ¤fer",
            "email": "mail@michael-schaefer.eu",
            "homepage": "https://michael-schaefer.eu"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
